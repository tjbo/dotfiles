
# first level is cmd 

# alt + t = dev1
# alt + / = settings
# alt + c = chrome
# alt + s = safari
# alt + p = pages
# alt + n = notes
# alt + whatever is reserved for apps on each desktop

# alt + shift will move an app to that workspace
# move the focused window to a certain space
shift + alt - t : yabai -m window --space 1
shift + alt - c : yabai -m window --space 2
shift + alt - b : yabai -m window --space 3
shift + alt - p : yabai -m window --space 4
shift + alt - n : yabai -m window --space 5
shift + alt - 6 : yabai -m window --space 6
shift + alt - 7 : yabai -m window --space 7
shift + alt - 8 : yabai -m window --space 8
shift + alt - s : yabai -m window --space 11 

# adjust focus
cmd + alt - j : yabai -m window --focus south
cmd + alt - k : yabai -m window --focus north 
cmd + alt - l : yabai -m window --focus east 
cmd + alt - h : yabai -m window --focus west 

# flip windows 
cmd + alt - y : yabai -m space --mirror y-axis

# toggle window float
cmd + alt - t : yabai -m window --toggle float --grid
4:4:1:1:2:2

# maximize
cmd + alt - m : yabai -m window --toggle zoom-fullscreen

# even space
cmd + alt - e : yabai -m space --balance 

# resize
# alt - 0x18: yabai -m window --resize right:5:-5
# alt - 0x1B: yabai -m window --resize right:-5:5




